<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_account_financial_report_form" model="ir.ui.view">
            <field name="name">account.financial.report.form (account_financial_report_prefix)</field>
            <field name="model">account.financial.report</field>
            <field name="inherit_id" ref="account.view_account_financial_report_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook[1]" position="after">
                    <group name="pfx" attrs="{'invisible': [('type','not in',['account_prefix'])]}">
                        <field name="account_prefix_ids" widget="many2many_tags"/>
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
